@{
    ViewBag.Title = "FriendLampList";
}
<body background="/Content/Blue_Light.jpg" style="background-size:cover;background-attachment:fixed">
    <div align="center">
        <h2 style="color:white">This Friend Liked These Lamps</h2>

        @*@ViewBag.FriendLampList
            @ViewBag.FriendLampListCount*@
        <div>
            <table>
                @foreach (var x in ViewBag.FriendLampListings)
            {

                    <td><a href="@x.ItemSearchURL" type="image" alt="Lamp Bae" target="_blank"> <img src="@x.Image" /> </a></td>
                }
            </table>
        </div>
    </div>
    </body>